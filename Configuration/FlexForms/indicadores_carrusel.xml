<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
    </meta>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>General</sheetTitle>
                    <sheetDescription>General Description</sheetDescription>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.indicadores_a_mostrar>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:indicadores_carrusel/Resources/Private/Language/locallang_indicadorescarruselbe.xlf:plugin_indicadorescarrusel_estadisticas</label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>tx_indicadorescarrusel_domain_model_indicador</allowed>
                                <size>5</size>
                                <maxitems>100</maxitems>
                                <minitems>5</minitems>
                                <show_thumbs>0</show_thumbs>
                            </config>
                        </TCEforms>
                    </settings.indicadores_a_mostrar>



                    <settings.texto_ver_mas>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:indicadores_carrusel/Resources/Private/Language/locallang_indicadorescarruselbe.xlf:plugin_indicadorescarrusel_texto_btnvermas</label>
                            <config>
                                <type>input</type>
                             </config>
                        </TCEforms>
                    </settings.texto_ver_mas>

      

                    <settings.link_ver_mas>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:indicadores_carrusel/Resources/Private/Language/locallang_indicadorescarruselbe.xlf:plugin_indicadorescarrusel_link_btnvermas</label>
                            <config>
                                <type>input</type>
                                <renderType>inputLink</renderType>
                             </config>
                        </TCEforms>
                    </settings.link_ver_mas>

                     <settings.periodo>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:indicadores_carrusel/Resources/Private/Language/locallang_indicadorescarruselbe.xlf:plugin_indicadorescarrusel_periodo</label>
                            <config>
                                <type>input</type>
                                <eval>int</eval>
                                <range>
                                    <lower>0</lower>
                                    <upper>60000</upper>
                                </range>
                             </config>
                        </TCEforms>
                    </settings.periodo>


                </el>
            </ROOT>
        </sDEF>
    </sheets>
</T3DataStructure>
